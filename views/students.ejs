<%- include('./partials/style.ejs') %>
    <h4>Students page</h4>
    <table>
        <tr>
            <th>id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>email</th>
            <th>gender</th>
            <th>birth_date</th>
            <th>Options</th>
        </tr>
        <% for(let i=0; i <data.length; i++) {%>
            <tr>
                <td>
                    <%= data[i].id %>
                </td>
                <td>
                    <%= data[i].first_name %>
                </td>
                <td>
                    <%= data[i].last_name %>
                </td>
                <td>
                    <%= data[i].email %>
                </td>
                <td>
                    <%= data[i].gender %>
                </td>
                <td>
                    <%= data[i].birth_date %>
                </td>
                <td>
                    <a href="students/<%= data[i].id %>/edit/">edit |</a>
                    <a href="students/<%= data[i].id %>/delete/">delete</a>
                </td>

            </tr>
            <%}%>
    </table><br><br>
    <%- include('./partials/footer.ejs') %>